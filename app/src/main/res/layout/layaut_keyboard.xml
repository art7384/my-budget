<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/contener"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/layautKeybord_Button_7"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="7" />

                <Button
                    android:id="@+id/layautKeybord_Button_8"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="8" />

                <Button
                    android:id="@+id/layautKeybord_Button_9"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="9" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/layautKeybord_Button_4"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="4" />

                <Button
                    android:id="@+id/layautKeybord_Button_5"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="5" />

                <Button
                    android:id="@+id/layautKeybord_Button_6"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="6" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/layautKeybord_Button_1"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="1" />

                <Button
                    android:id="@+id/layautKeybord_Button_2"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="2" />

                <Button
                    android:id="@+id/layautKeybord_Button_3"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="3" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/layautKeybord_Button_zp"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="," />

                <Button
                    android:id="@+id/layautKeybord_Button_0"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="0" />

                <Button
                    android:id="@+id/layautKeybord_Button_rav"
                    style="@style/Button.Cal"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:padding="0dp"
                    android:text="=" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="84dp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/contOpiration"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:background="#33ffffff"
            android:orientation="vertical">

            <Button
                android:id="@+id/c"
                style="@style/Button.Cal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:padding="0dp"
                android:text="c"
                android:textSize="22sp" />

            <Button
                android:id="@+id/delit"
                style="@style/Button.Cal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:padding="0dp"
                android:text="รท"
                android:textSize="22sp"
                android:visibility="invisible" />

            <Button
                android:id="@+id/mult"
                style="@style/Button.Cal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:padding="0dp"
                android:text="ร"
                android:textSize="22sp"
                android:visibility="invisible" />

            <Button
                android:id="@+id/min"
                style="@style/Button.Cal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:padding="0dp"
                android:text="-"
                android:textSize="22sp"
                android:visibility="invisible" />

            <Button
                android:id="@+id/cplus"
                style="@style/Button.Cal"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:padding="0dp"
                android:text="+"
                android:textSize="22sp"
                android:visibility="gone" />

            <View
                android:layout_width="1dp"
                android:layout_height="84dp" />

        </LinearLayout>

        <!--CheckBox
            android:id="@+id/done"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="16dp"
            android:fontFamily="@font/roboto_regular"
            android:text="@string/already_spent_received"
            android:textColor="@color/text_cal" /-->
    </LinearLayout>

    <Button
        android:layout_centerHorizontal="true"
        android:id="@+id/selectCategory"
        style="@style/MyButtonStyle.Colored"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="16dp"
        android:layout_marginHorizontal="32dp"
        android:text="@string/select_category" />


</RelativeLayout>